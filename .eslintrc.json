{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": "off",
    "consistent-return": "off",
    "comma-dangle": "off",
    "no-underscore-dangle": "off"
  }
}
